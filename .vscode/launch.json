{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "protocol": "inspector",
      "request": "launch",
      "name": "jest test",
      "program": "${workspaceRoot}/node_modules/jest/bin/jest",
      "args": [
        "${fileBasenameNoExtension}",
